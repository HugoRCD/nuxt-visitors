{
  "name": "nuxt-visitors",
  "version": "1.2.2",
  "description": "Add real-time visitor tracking to your Nuxt app with one line of code. WebSocket made easy",
  "license": "Apache-2.0",
  "type": "module",
  "bugs": {
    "url": "https://github.com/HugoRCD/nuxt-visitors/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/nuxt-visitors.git"
  },
  "funding": "https://github.com/sponsors/HugoRCD",
  "author": "HugoRCD",
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "3.17.3",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@hrcd/eslint-config": "^3.0.3",
    "@nuxt/devtools": "^2.4.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "3.17.3",
    "@nuxt/test-utils": "^3.18.0",
    "@types/node": "latest",
    "automd": "^0.4.0",
    "eslint": "^9.26.0",
    "nuxt": "3.17.3",
    "typescript": "5.8.3",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  },
  "packageManager": "pnpm@10.10.0"
}
