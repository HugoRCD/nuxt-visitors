{
  "name": "nuxt-visitors",
  "version": "1.1.0",
  "description": "Add real-time visitor tracking to your Nuxt app with one line of code. WebSocket made easy",
  "license": "Apache-2.0",
  "type": "module",
  "bugs": {
    "url": "https://github.com/HugoRCD/nuxt-visitors/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/nuxt-visitors.git"
  },
  "funding": "https://github.com/sponsors/HugoRCD",
  "author": "HugoRCD",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.3"
  },
  "devDependencies": {
    "@hrcd/eslint-config": "^2.1.1",
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/eslint-config": "^0.7.5",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.3",
    "@nuxt/test-utils": "^3.15.4",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.19.0",
    "nuxt": "3.15.3",
    "typescript": "5.7.2",
    "vitest": "^3.0.4",
    "vue-tsc": "^2.2.0",
    "automd": "^0.3.12"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
